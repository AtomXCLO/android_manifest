<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- AOSP -->
  <remote name="aosp"
          fetch="https://android.googlesource.com"
          review="https://android-review.googlesource.com/"
          revision="refs/tags/android-12.1.0_r5"/>

  <!-- CLO -->
  <remote name="clo_system"
          fetch="https://git.codelinaro.org/clo/la"
          review="codelinaro.org"/>

  <remote name="clo_vendor"
          fetch="https://git.codelinaro.org/clo/la"
          review="codelinaro.org"
          revision="refs/tags/LA.UM.9.14.r1-19800.01-LAHAINA.QSSI12.0"/>

  <default revision="refs/tags/LA.QSSI.12.0.r1-07900-qssi.0"
           remote="clo_system"/>

  <include name="system.xml"/>
  <include name="vendor.xml"/>
  <include name="obsolete.xml"/>

  <!-- AtomX -->
  <remote  name="atomx"
           fetch=".."
           review="https://review.atomx-devs.org/"
           revision="sc-v2"/>

  <remote  name="atomx-gitlab"
           fetch="https://gitlab.com/AtomXCLO"
           revision="sc-v2"/>

  <remote  name="atomx-devices"
           fetch="https://github.com/AtomXCLO-Devices"
           revision="sc-v2"/>

  <include name="atomx.xml"/>

  <!-- EXTERNAL -->
  <remote  name="github"
           fetch="https://github.com" />

  <remote  name="gitlab"
           fetch="https://gitlab.com" />

  <remote name="pixel-gitlab"
          fetch="https://gitlab.pixelexperience.org/android" />

  <include name="external.xml"/>

</manifest>
